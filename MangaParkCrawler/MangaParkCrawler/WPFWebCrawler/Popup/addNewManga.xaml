<Window x:Class="WPFWebCrawler.Popup.addNewManga"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="addNewManga" Height="203" Width="578">
    <Grid>
        <RadioButton Content="Manga Park" Height="16" HorizontalAlignment="Left" Margin="97,12,0,0" Name="rdoWeb_MangaPark" VerticalAlignment="Top" IsChecked="True" />
        <Label Content="Link :" Height="28" HorizontalAlignment="Left" Margin="17,42,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="21" HorizontalAlignment="Left" Margin="65,45,0,0" Name="tbLink" VerticalAlignment="Top" Width="465" TextChanged="tbLink_TextChanged" />
        <Button Content="Add" Height="23" HorizontalAlignment="Left" Margin="374,118,0,0" Name="btnAdd" VerticalAlignment="Top" Width="75" Click="btnAdd_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="455,118,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" />
        <Label Content="Manga Name :" Height="28" HorizontalAlignment="Left" Margin="17,84,0,0" Name="label2" VerticalAlignment="Top" />
        <Label Content="" Height="28" HorizontalAlignment="Left" Margin="111,84,0,0" Name="lblMangaKey" VerticalAlignment="Top" FontWeight="Bold"  />
    </Grid>
</Window>
